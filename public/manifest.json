{
    "name": "autoSIAK",
    "author": "deadManAlive",
    "description": "Automated SIAK-NG Agent",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "declarativeContent"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
    },
    "action": {
        "default_title": "autoSIAK",
        "default_icon": {
            "16": "icons/icon_16.png",
            "32": "icons/icon_32.png",
            "48": "icons/icon_48.png",
            "128": "icons/icon_128.png"
        },
        "default_popup": "popup.html"
    },
    "options_page": "options.html"
}